 class="line">   right,</span><br><span class="line">&#125;;</span><br><span class="line">struct snake//è›‡çš„ç»“æ„</span><br><span class="line">&#123;</span><br><span class="line">  int size;//è›‡çš„å­—èŠ‚</span><br><span class="line">  int dir;//è›‡çš„æ–¹å‘</span><br><span class="line">  int speed;//ç§»åŠ¨é€Ÿåº¦</span><br><span class="line">  POINT coor[Coords];//åæ ‡</span><br><span class="line">&#125;snake;</span><br><span class="line">struct food</span><br><span class="line">&#123;</span><br><span class="line">int x;</span><br><span class="line">int y;</span><br><span class="line">int r;</span><br><span class="line">bool flag;//åˆ¤å®šæ˜¯å¦è¢«åƒ</span><br><span class="line">DWORD color;</span><br><span class="line">&#125;food;</span><br><span class="line">void loadrescource()</span><br><span class="line">&#123;</span><br><span class="line">IMAGE background;//å®šä¹‰ä¸€ä¸ªå›¾ç‰‡å.</span><br><span class="line">loadimage(&amp;background, &quot;./picture1/true.jfif&quot;,700,500);//ä»å›¾ç‰‡æ–‡ä»¶è·å–å›¾åƒ</span><br><span class="line">putimage(0, 0, &amp;background);//ç»˜åˆ¶å›¾åƒåˆ°å±å¹•ï¼Œå›¾ç‰‡å·¦ä¸Šè§’åæ ‡ä¸º(0,0)</span><br><span class="line">&#125;</span><br><span class="line">void gameinit()//åˆå§‹åŒ–</span><br><span class="line">&#123;</span><br><span class="line">isplay = true;</span><br><span class="line">mciSendString(&quot;open ./music/7895.mp3 alias bgm&quot;, 0, 0, 0);</span><br><span class="line">mciSendString(&quot;play bgm repeat&quot;, 0, 0, 0);</span><br><span class="line">//initåˆå§‹åŒ– graph å›¾å½¢çª—å£</span><br><span class="line">initgraph(length,high);</span><br><span class="line">srand(GetTickCount());//éšæœºæ•°ç§å­</span><br><span class="line">//è®¾æ—¶é—´ä¸ºéšæœºæ•°,è·å–ç³»ç»Ÿå¼€æœºåˆ°ç°åœ¨çš„æ¯«ç§’æ•°</span><br><span class="line">//åˆå§‹åŒ–è›‡ï¼Œä¸€å¼€å§‹3èŠ‚</span><br><span class="line">snake.size = 3; </span><br><span class="line">    snake.speed =5;</span><br><span class="line">snake.dir=right;//æ–¹å‘</span><br><span class="line">for (int i = 0; i &lt;snake.size; i++) &#123;</span><br><span class="line">snake.coor[i].x = 70-15*i;</span><br><span class="line">    snake.coor[i].y = 10;</span><br><span class="line">&#125;</span><br><span class="line">//åˆå§‹åŒ–é£Ÿç‰©</span><br><span class="line">food.x = rand() % (length-100)+25;//éšæœºäº§ç”Ÿä¸€ä¸ªæ•´æ•°çš„å‡½æ•°ï¼Œéœ€è¦éšæœºæ•°ç§å­</span><br><span class="line">food.y = rand() % (high - 100)+25;</span><br><span class="line">food.color = RGB(rand()%256,rand()%256,rand()%256);</span><br><span class="line">food.r = rand() % 10 +10;</span><br><span class="line">food.flag = true;</span><br><span class="line">&#125;</span><br><span class="line">void gamedraw()//ç»˜åˆ¶è›‡</span><br><span class="line">&#123;</span><br><span class="line">//sprintf(Time,&quot;%d&quot;,pp);</span><br><span class="line">BeginBatchDraw();//åŒç¼“å­˜</span><br><span class="line">//setbkcolor(RGB(28, 115, 119));</span><br><span class="line">cleardevice();//æ¸…ç©ºç»˜å›¾è®¾å¤‡</span><br><span class="line">loadrescource();</span><br><span class="line">outtextxy(610, 10, &quot;åˆ†æ•°:&quot;);</span><br><span class="line">outtextxy(650, 10, num);</span><br><span class="line">outtextxy(540, 10, &quot;éŸ³ä¹:&quot;);</span><br><span class="line">if (isplay)</span><br><span class="line">&#123;</span><br><span class="line">         outtextxy(585, 10, &quot;å¼€&quot;);</span><br><span class="line">&#125;</span><br><span class="line">else</span><br><span class="line">&#123;</span><br><span class="line">outtextxy(585, 10, &quot;å…³&quot;);</span><br><span class="line">&#125;</span><br><span class="line">setfillcolor(BLUE);//è®¾ç½®è›‡èº«é¢œè‰²ä¸ºè“è‰²</span><br><span class="line">for (int i = 1; i &lt;snake.size; i++) &#123;</span><br><span class="line">solidcircle(snake.coor[i].x, snake.coor[i].y, 6);//xè½´ï¼Œyè½´ï¼ŒåŠå¾„</span><br><span class="line">&#125;</span><br><span class="line">setfillcolor(RED);//è®¾ç½®è›‡å¤´é¢œè‰²ä¸ºçº¢è‰²</span><br><span class="line">solidcircle(snake.coor[0].x, snake.coor[0].y, 6);</span><br><span class="line">if (food.flag==true)</span><br><span class="line">&#123;</span><br><span class="line">solidcircle(food.x, food.y, food.r);</span><br><span class="line">&#125;</span><br><span class="line">EndBatchDraw();</span><br><span class="line">&#125;</span><br><span class="line">void snakemove() &#123;//ç§»åŠ¨è›‡</span><br><span class="line">for (int i = snake.size-1; i &gt;0; i--)//åé¢åˆ°å‰é¢ä¾æ¬¡å‘å‰èµ°</span><br><span class="line">&#123;</span><br><span class="line">snake.coor[i]=snake.coor[i - 1];</span><br><span class="line"> &#125;</span><br><span class="line">switch (snake.dir)</span><br><span class="line">&#123;</span><br><span class="line">case up://è¿›è¡Œæ–¹å‘æ”¹å˜</span><br><span class="line">snake.coor[0].y-=snake.speed;</span><br><span class="line">if (snake.coor[0].y +6&lt;= 0) </span><br><span class="line">&#123;</span><br><span class="line">snake.coor[0].y = high;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case down://åŒä¸Š</span><br><span class="line">snake.coor[0].y+=snake.speed;</span><br><span class="line">if (snake.coor[0].y + 6 &gt;= high)</span><br><span class="line">&#123;</span><br><span class="line">snake.coor[0].y = 0;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case left://åŒä¸Š</span><br><span class="line">snake.coor[0].x-=snake.speed;</span><br><span class="line">if (snake.coor[0].x + 6 &lt;= 0)</span><br><span class="line">&#123;</span><br><span class="line">snake.coor[0].x = length;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case right://åŒä¸Š</span><br><span class="line">snake.coor[0].x+=snake.speed;</span><br><span class="line">if (snake.coor[0].x+ 6 &gt;=length)</span><br><span class="line">&#123;</span><br><span class="line">snake.coor[0].x = 0;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">void keycontrol()</span><br><span class="line">&#123;//é€šè¿‡æŒ‰é”®è¿›è¡Œæ§åˆ¶</span><br><span class="line">if (_kbhit())//åˆ¤æ–­æœ‰æ²¡æœ‰æŒ‰é”®ï¼Œå¦‚æœæœ‰å°±ä¸ºçœŸ</span><br><span class="line">&#123;</span><br><span class="line">    switch (_getch())</span><br><span class="line">  &#123;</span><br><span class="line">//72 80 75 77 ä¸Šä¸‹å·¦å³é”®å€¼ </span><br><span class="line">case &#x27;w&#x27;:</span><br><span class="line">case &#x27;W&#x27;:</span><br><span class="line">case 72:</span><br><span class="line">if (snake.dir != down) </span><br><span class="line">&#123;</span><br><span class="line">                     snake.dir = up;</span><br><span class="line">&#125;</span><br><span class="line">//æ”¹å˜æ–¹å‘</span><br><span class="line">break;</span><br><span class="line">case &#x27;s&#x27;:</span><br><span class="line">case &#x27;S&#x27;:</span><br><span class="line">case 80:</span><br><span class="line">if (snake.dir != up)</span><br><span class="line">&#123;</span><br><span class="line">   snake.dir = down;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case &#x27;a&#x27;:</span><br><span class="line">case &#x27;A&#x27;:</span><br><span class="line">case 75:</span><br><span class="line">if (snake.dir != right)</span><br><span class="line">&#123;</span><br><span class="line">snake.dir = left;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case &#x27;d&#x27;:</span><br><span class="line">case &#x27;D&#x27;:</span><br><span class="line">case 77:</span><br><span class="line">if (snake.dir != left)</span><br><span class="line">&#123;</span><br><span class="line">snake.dir = right;</span><br><span class="line">&#125;</span><br><span class="line">break;</span><br><span class="line">case &#x27; &#x27;:   //æŒ‰ç©ºæ ¼æš‚åœ</span><br><span class="line">if (isplay)</span><br><span class="line">&#123;</span><br><span class="line">mciSendString(&quot;pause  bgm&quot;, 0, 0, 0);</span><br><span class="line">isplay = !isplay;</span><br><span class="line">outtextxy(585, 10, &quot;å…³&quot;);</span><br><span class="line">&#125;</span><br><span class="line">else</span><br><span class="line">&#123;</span><br><span class="line">mciSendString(&quot;play  bgm repeat&quot;, 0, 0, 0);</span><br><span class="line">isplay = !isplay;</span><br><span class="line">outtextxy(585, 10, &quot;å¼€&quot;);</span><br><span class="line">&#125;</span><br><span class="line">while (1)</span><br><span class="line">&#123;</span><br><span class="line">if (_getch() == &#x27; &#x27;)</span><br><span class="line">&#123;</span><br><span class="line">outtextxy(585, 10, &quot;å¼€&quot;);</span><br><span class="line">return;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">     &#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">void eatfood()</span><br><span class="line">&#123;</span><br><span class="line">sprintf(num,&quot;%d&quot;,cnt);</span><br><span class="line">if (food.flag == true &amp;&amp; snake.coor[0].x &gt;= food.x - food.r &amp;&amp; snake.coor[0].x &lt;= food.x + food.r</span><br><span class="line">&amp;&amp; snake.coor[0].y &gt;= food.y - food.r &amp;&amp; snake.coor[0].y &lt;= food.y + food.r)</span><br><span class="line">&#123;//åˆ¤æ–­åƒåˆ°é£Ÿç‰©</span><br><span class="line">food.flag = false;</span><br><span class="line">snake.size++;</span><br><span class="line">//snake.speed++;</span><br><span class="line">if (food.r&gt;=8)</span><br><span class="line">&#123;</span><br><span class="line">        cnt+=1;</span><br><span class="line">&#125;</span><br><span class="line">else if (food.r &lt; 8)</span><br><span class="line">&#123;</span><br><span class="line">cnt += 2;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">if (food.flag == false)//åˆ·æ–°é£Ÿç‰©</span><br><span class="line">&#123;</span><br><span class="line">food.x = rand() % length;//éšæœºäº§ç”Ÿä¸€ä¸ªæ•´æ•°çš„å‡½æ•°ï¼Œéœ€è¦éšæœºæ•°ç§å­</span><br><span class="line">food.y = rand() % high;</span><br><span class="line">food.color = RGB(rand() % 256, rand() % 256, rand() % 256);</span><br><span class="line">food.r = rand() % 10 + 5;</span><br><span class="line">food.flag = true;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">void cheak()//åƒåˆ°è‡ªå·±æ­»äº¡</span><br><span class="line">&#123;</span><br><span class="line">for (int i = 1; i &lt; snake.size; i++)</span><br><span class="line">&#123;</span><br><span class="line">if (snake.coor[0].x == snake.coor[i].x &amp;&amp;</span><br><span class="line">snake.coor[0].y == snake.coor[i].y)</span><br><span class="line">&#123;</span><br><span class="line">gameinit();</span><br><span class="line">cnt = 0;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">int main()</span><br><span class="line">&#123;</span><br><span class="line">gameinit();</span><br><span class="line">while (1) </span><br><span class="line">&#123; </span><br><span class="line">  gamedraw();</span><br><span class="line">  snakemove();</span><br><span class="line">  keycontrol();</span><br><span class="line">  eatfood();</span><br><span class="line">  Sleep(20);//å»¶è¿Ÿ20æ¯«ç§’</span><br><span class="line">  cheak();</span><br><span class="line">&#125;</span><br><span class="line">return 0;</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure><h2 id="åˆ°æ­¤ä¸ºæ­¢ï¼Œæ„Ÿæƒ³å¤§å®¶è§‚çœ‹"><a href="#åˆ°æ­¤ä¸ºæ­¢ï¼Œæ„Ÿæƒ³å¤§å®¶è§‚çœ‹" class="headerlink" title="åˆ°æ­¤ä¸ºæ­¢ï¼Œæ„Ÿæƒ³å¤§å®¶è§‚çœ‹"></a>åˆ°æ­¤ä¸ºæ­¢ï¼Œæ„Ÿæƒ³å¤§å®¶è§‚çœ‹</h2><h2 id="ps-åˆšæ­çš„åšå®¢ï¼Œè¿˜æ²¡æå¥½ï¼Œå…ˆå‡‘åˆç”¨"><a href="#ps-åˆšæ­çš„åšå®¢ï¼Œè¿˜æ²¡æå¥½ï¼Œå…ˆå‡‘åˆç”¨" class="headerlink" title="ps:åˆšæ­çš„åšå®¢ï¼Œè¿˜æ²¡æå¥½ï¼Œå…ˆå‡‘åˆç”¨"></a><del>ps:åˆšæ­çš„åšå®¢ï¼Œè¿˜æ²¡æå¥½ï¼Œå…ˆå‡‘åˆç”¨</del></h2><p>æ­¤ä»£ç ä»…ä¾›å‚è€ƒï¼Œå¸®åŠ©å¤§å®¶ç†è§£ã€‚ï¼ˆæ¯•ç«Ÿæˆ‘è‡ªå·±å¼€å§‹å¼„çš„æ—¶å€™ä»€ä¹ˆéƒ½çœ‹ä¸æ‡‚ï¼ŒèŠ±äº†å¥½ä¹…æ‰ä¼šçš„ï¼Œå¸Œæœ›è¿™ä¸ªèƒ½å¸®åŠ©å¤§å®¶ï¼‰</p>]]></content>
      
      
      
